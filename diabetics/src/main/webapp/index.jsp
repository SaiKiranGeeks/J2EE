	<!-- Masthead-->
	<header class="masthead bg-primary text-white text-center">
		
			<!-- Masthead Heading-->
			<h1 class="masthead-heading text-uppercase mb-0">Enter Your Diabetes Information</h1>
			
	</header>
	
	<form action="Diabetes" method="post">
		<input type="hidden" id="status"
		value="<%=request.getAttribute("status")%>">
		
		<input type="hidden" name="name" value="<%= session.getAttribute("name") %>">
		
        <fieldset>
          <legend><center>Date and Time</center></legend>
          <div class="form-group row">
            <label for="date" class="col-sm-3 col-form-label">Date:</label>
            <div class="col-sm-9">
              <input type="date" id="date" name="date" class="form-control" required = "required">
            </div>
          </div>
          <div class="form-group row">
            <label for="morningtime" class="col-sm-3 col-form-label">Test Time In Morning:</label>
            <div class="col-sm-9">
              <input type="time" id="morningtime" name="morningtime" class="form-control">
            </div>
          </div>
          <div class="form-group row">
            <label for="morning" class="col-sm-3 col-form-label">Fasting Blood Sugar Level:</label>
            <div class="col-sm-9">
              <input type="number" id="morning" name="morning" class="form-control" placeholder="mg/dL">
            </div>
          </div>
          <div class="form-group row">
            <label for="breakfasttime" class="col-sm-3 col-form-label">Breakfast Time:</label>
            <div class="col-sm-9">
              <input type="time" id="breakfasttime" name="breakfasttime" class="form-control">
            </div>
          </div>
          <div class="form-group row">
            <label for="breakfastfood" class="col-sm-3 col-form-label">Breakfast Food:</label>
            <div class="col-sm-9">
              <input type="text" id="breakfastfood" name="breakfastfood" class="form-control">
            </div>
          </div>
          <div class="form-group row">
            <label for="lunchtime" class="col-sm-3 col-form-label">Lunch Time:</label>
            <div class="col-sm-9">
              <input type="time" id="lunchtime" name="lunchtime" class="form-control">
            </div>
          </div>
          <div class="form-group row">
            <label for="lunchfood" class="col-sm-3 col-form-label">Lunch Food:</label>
            <div class="col-sm-9">
              <input type="text" id="lunchfood" name="lunchfood" class="form-control">
            </div>
          </div>
          <div class="form-group row">
            <label for="dinnertime" class="col-sm-3 col-form-label">Dinner Time:</label>
            <div class="col-sm-9">
              <input type="time" id="dinnertime" name="dinnertime" class="form-control">
            </div>
          </div>
          <div class="form-group row">
            <label for="dinnerfood" class="col-sm-3 col-form-label">Dinner Food:</label>
            <div class="col-sm-9">
              <input type="text" id="dinnerfood" name="dinnerfood" class="form-control">
            </div>
          </div>
           <div class="form-group row">
            <label for="nighttime" class="col-sm-3 col-form-label">Test Time In night:</label>
            <div class="col-sm-9">
              <input type="time" id="nighttime" name="nighttime" class="form-control">
            </div>
          </div>
          <div class="form-group row">
            <label for="night" class="col-sm-3 col-form-label">Sugar Level after two hours of taking food:</label>
            <div class="col-sm-9">
              <input type="number" id="night" name="night" class="form-control" placeholder="mg/dL">
            </div>
          </div>
        </fieldset>
    	<center>
			<input type="submit" value="submit">
		</center>
	</form>
	<script>
	var status = document.getElementById("status").value;
	if (status == "failed") {
		swal("Sorry", "Wrong Username or Password", "error");
	} else if (status == "invalidEmail") {
		swal("Sorry", "Please Enter Correct Email Address", "error");
	} else if (status == "invalidUpwd") {
		swal("Sorry", "Please Enter Password", "error");
	} else if (status == "resetSuccess") {
		swal("Congrats", "Password Reset Successfully", "success");
	} else if (status == "resetFailed") {
		swal("Sorry", "Password Reset Failed", "error");
	}
	 else if (status == "sucesslogin") {
		 swal("Congrats", "Login Successfully", "success");
			
		}
	
	</script>
